<div class="breadcrumbs">
    {{#each breadcrumbs}} {{#if href}}
    <a href="{{href}}">{{text}}</a> / {{else}}
    <span>{{text}}</span>
    {{/if}} {{/each}}
</div>

<div class="content">
    {{#each list}}
    <div class="commit">
        <div class="commit__info">
            <span class="commit__author">{{author}}</span>
            <span class="commit__date">{{timestamp}}</span>
        </div>
        <div class="commit__msg">
            {{msg}}
        </div>
        <div class="commit__link">
            <a id="commit-{{hash}}" href="{{href}}">{{hash}}</a>
        </div>
    </div>
    {{/each}}
</div>